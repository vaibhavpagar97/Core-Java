package Day2;

// Scanner class
import java.util.*;

public class Array1 {
	public static void main(String[] args) {
		
		//decalre scanner
		Scanner sc = new Scanner(System.in);
		int size = sc.nextInt();          //declare int for size of array
		int arr[] = new int[size];        //declare array
		
		for (int i = 0 ; i<size;i++) {     //for loop to get array input
			arr[i] = sc.nextInt();
		}
		int j ;
		for(j = 0 ; j<size;j++) {           // for loop to print array element
			System.out.println(arr[j]);
		}
	}
	

}
